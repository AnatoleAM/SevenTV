{
	"manifest_version": 2,
	"name": "7TV",
	"description": "A third party emote extension for Twitch",
	"version": "1.5.0",
	"page_action": {
		"default_icon": "icon128.png"
	},
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"web_accessible_resources": [
		"page.js",
		"styles/Style.css",
		"image/*.webp"
	],
	"permissions": [
		"*://*.twitch.tv/*",
		"storage", "unlimitedStorage"
	],
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	}
}
