{
	"manifest_version": 2,
	"name": "SevenTV",
	"description": "Third party thing",
	"version": "0.1",
	"page_action": {
		"default_icon": "icon.png"
	},
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"content_scripts": [
		{
			"matches": ["*://www.twitch.tv/*"],
			"js": [
				"content.js"
			]
		}
	],
	"permissions": [
		"*://www.twitch.tv/*",
		"tabs",
        "activeTab",
        "webNavigation"
	]
}
